{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyHomePage": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "backendUri": "https://aramizblog.z16.web.core.windows.net/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/content/{*restOfPath}"
      },
      "backendUri": "https://aramizblog.z16.web.core.windows.net/{*restOfPath}"
    },
    "Favicon": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/{file}.{ending}"
      },
      "backendUri": "https://aramizblog.z16.web.core.windows.net/{file}.{ending}"
    },
    "Blogapi": {
      "matchCondition": {
        "route": "/api/blog/{*url}"
      },
      "backendUri": "https://aramizblog.azurewebsites.net/api/{url}"
    }
  }
}